{
  "name": "@helia/delegated-routing-v1-http-api-server",
  "version": "1.0.5",
  "description": "A Delegated Routing V1 HTTP API server powered by Helia",
  "license": "Apache-2.0 OR MIT",
  "homepage": "https://github.com/ipfs/helia-delegated-routing-v1-http-api/tree/master/packages/server#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipfs/helia-delegated-routing-v1-http-api.git"
  },
  "bugs": {
    "url": "https://github.com/ipfs/helia-delegated-routing-v1-http-api/issues"
  },
  "keywords": [
    "IPFS"
  ],
  "type": "module",
  "types": "./dist/src/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/src/*",
        "dist/src/*/index"
      ],
      "src/*": [
        "*",
        "dist/*",
        "dist/src/*",
        "dist/src/*/index"
      ]
    }
  },
  "files": [
    "src",
    "dist",
    "!dist/test",
    "!**/*.tsbuildinfo"
  ],
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./routes": {
      "types": "./dist/src/routes/index.d.ts",
      "import": "./dist/src/routes/index.js"
    }
  },
  "eslintConfig": {
    "extends": "ipfs",
    "parserOptions": {
      "project": true,
      "sourceType": "module"
    }
  },
  "scripts": {
    "clean": "aegir clean",
    "lint": "aegir lint",
    "dep-check": "aegir dep-check -i sinon",
    "build": "aegir build --bundle false",
    "test": "aegir test -t node",
    "test:node": "aegir test -t node --cov",
    "release": "aegir release"
  },
  "dependencies": {
    "@fastify/cors": "^8.3.0",
    "@libp2p/interface": "^0.1.2",
    "@libp2p/peer-id": "^3.0.3",
    "fastify": "^4.17.0",
    "ipns": "^7.0.1",
    "multiformats": "^13.0.0",
    "raw-body": "^2.5.2"
  },
  "devDependencies": {
    "@helia/interface": "^2.0.0",
    "@libp2p/peer-id-factory": "^3.0.5",
    "@multiformats/multiaddr": "^12.1.3",
    "@types/sinon": "^17.0.0",
    "aegir": "^42.1.1",
    "sinon": "^17.0.0",
    "sinon-ts": "^2.0.0"
  }
}
